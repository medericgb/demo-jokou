<div>
  <button>

 
</div>
<table>
  <thead>
    <tr>
      <th>name</th>
      <th>description</th>
      <th>price</th>
      <th>weight</th>
      <th>categories</th>
      <th>store</th>
    </tr>
  </thead>
  <tbody>
  <% @products.each do |product| %>
    <tr>
      <td><%= product.name %></td>
      <td><%= product.description %></td>
      <td><%= product.price %></td>
      <td><%= product.weight %></td>
      <td><%= product.all_categories %></td>
      <td><%= product.store.name %></td>
    </tr>
  <% end %>
  </tbody>
</table>
