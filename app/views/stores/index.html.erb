<div>

  <button>
    <%= link_to 'Logout', destroy_user_session_path, method: :delete %>
  </button>
</div>
<table>
  <thead>
    <tr>
      <th>name</th>
      <th>description</th>
      <th>city</th>
      <th>town</th>

    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @store.name %></td>
      <td><%= @store.description %></td>
      <td><%= @store.city %></td>
      <td><%= @store.town %></td>
      <td>
        <button class="bg-green-500 hover:bg-green-700 text-white font-bold py-2 px-4 rounded">
          <%= link_to 'edit', edit_user_store_path(current_user, @store)%>
        </button>
      </td>
      <td>
        <button class="bg-red-800 hover:bg-red-700 text-white font-bold py-2 px-4 rounded">
          <%= link_to 'delete', user_store_path(current_user, @store), method: :delete, data: {confirm: 'Are you sure?'}%>
        </button>
      </td>
         <td>
          <button class="bg-blue-800 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded">
            <%= link_to 'Show', user_store_path(current_user, @store)%>
          </button>
      </td>
    </tr>
  </tbody>

</table>
