<h2><%= cart.store.name %></h2>
<% cart.line_items.each do |item| %>
  <%= render partial: "shared/cart_item", locals: { item: item } %>
<% end %>
<%= link_to "Delete cart", user_cart_path(current_user, cart), method: :delete, data: { confirm: "Are you sure ?"}%> | <%= link_to "Validate your cart" %>